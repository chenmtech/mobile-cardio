<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <style name="ViewStyleKey" parent="@android:style/TextAppearance.Small">
        <item name="android:layout_width">150dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:typeface">serif</item>
    </style>
    <style name="EditStyleValue" parent="@android:style/TextAppearance.Small">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:typeface">serif</item>
        <item name="android:paddingLeft">30dp</item>
    </style>
    <style name="Edit2StyleValue" parent="@android:style/TextAppearance.Small">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:typeface">serif</item>
    </style>
    <style name="RelativeStyle">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:paddingTop">10dp</item>
    </style>
    <style name="HeadRealativeStyle">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:textSize">20dp</item>
    </style>
    <style name="CheckStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:checked">false</item>
        <item name="android:clickable">false</item>
    </style>
    <style name="ECGViewStyle" parent="@android:style/TextAppearance.Small">
        <item name="android:layout_width">80dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:typeface">serif</item>
    </style>
</resources>